{
  "name": "t3monitor/t3monitoring",
  "type": "typo3-cms-extension",
  "description": "T3monitoring service",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "typo3/cms-core": "^11 || ^12",
    "typo3/cms-backend": "^11 || ^12",
    "typo3/cms-extbase": "^11 || ^12",
    "typo3/cms-extensionmanager": "^11 || ^12"
  },
  "require-dev": {
    "typo3/minimal": "^12",
    "typo3/testing-framework": "*"
  },
  "autoload": {
    "psr-4": {
      "T3Monitor\\T3monitoring\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "T3Monitor\\T3monitoring\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/Web",
      "extension-key": "t3monitoring"
    }
  }
}
